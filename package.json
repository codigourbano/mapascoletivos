{
	"name": "mapascoletivos",
	"description": "A collaborative mapping platform",
	"repository": {
		"type": "git",
		"url": "git://github.com/oeco/mapascoletivos.git"
	},
	"version": "0.1.0",
	"author": "Ecolab <gustavo.faleiros@gmail.com>",
	"contributors": [
		{
			"name": "Miguel Peixe",
			"email": "miguel@cardume.art.br"
		},
		{
			"name": "Vitor George",
			"email": "vitor.george@gmail.com"
		}
	],
	"engines": {
		"node": "0.10.x",
		"npm": "1.2.x"
	},
	"main": "web.js",
	"scripts": {
		"build": "./node_modules/.bin/napa && ./node_modules/.bin/browserify client/vendor.js | ./node_modules/.bin/uglifyjs -mt > public/vendor.js",
		"watch": "./node_modules/.bin/watchify client/app.js -o public/app.js -v & ( sleep 3 && NODE_PATH=./app/controllers NODE_ENV=development ./node_modules/.bin/nodemon web.js)",
		"start": "./node_modules/.bin/browserify client/app.js | ./node_modules/.bin/uglifyjs -mt > public/app.js & NODE_PATH=./app/controllers node web.js",
		"install": "npm run build",
		"test": "make test"
	},
	"dependencies": {
		"dotenv": "latest",
		"moment": "latest",
		"express": "latest",
		"express-namespace": "latest",
		"mysql": "latest",
		"mongoose": "latest",
		"mongoose-user": "latest",
		"mongoose-schema-extend": "latest",
		"crypto": "latest",
		"validator": "latest",
		"nodemailer": "latest",
		"passport": "latest",
		"passport-local": "latest",
		"passport-facebook": "latest",
		"passport-twitter": "latest",
		"passport-google-oauth": "latest",
		"connect-mongo": "latest",
		"connect-flash": "latest",
		"view-helpers": "latest",
		"jade": "latest",
		"async": "latest",
		"underscore": "latest",
		"forever": "latest",
		"less": "latest",
		"less-middleware": "~0.1.15",
		"lorem-ipsum": "latest",
		"imager": "latest",
		"leaflet": "latest",
		"mapbox.js": "latest",
		"jquery": "latest",
		"jquery-ui-browserify": "latest",
		"markdown": "latest",
		"grvtr": "latest",
		"napa": "latest",
		"browserify": "latest",
		"uglify-js": "latest",
		"unzip": "latest",
		"shapefile": "latest"
	},
	"devDependencies": {
		"watchify": "latest",
		"nodemon": "latest",
		"mocha": "latest",
		"should": "latest",
		"supertest": "latest",
		"factory-lady": "latest"
	},
	"napa": {
		"angular": "angular/bower-angular",
		"angular-route": "angular/bower-angular-route",
		"angular-sanitize": "angular/bower-angular-sanitize",
		"angular-resource": "angular/bower-angular-resource",
		"angular-animate": "angular/bower-angular-animate",
		"angular-elastic": "monospaced/angular-elastic",
		"angular-fitvids": "jimmyhillis/angular-fitvids",
		"angular-ui-utils": "angular-ui/ui-utils",
		"angular-ui-router": "angular-ui/ui-router",
		"angular-ui-sortable": "angular-ui/ui-sortable",
		"angular-modal": "miguelpeixe/angular-modal",
		"ngInfiniteScroll": "BinaryMuse/ngInfiniteScroll-bower",
		"eventable": "madebymany/eventable",
		"sir-trevor": "madebymany/sir-trevor-js"
	},
	"keywords": [
		"mapas",
		"mapascoletivos",
		"openstreetmap"
	]
}
